{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "esaCustom/camelCaseFunctions": "error",
        "esaCustom/noMomentLibrary": "error",
        "esaCustom/noConsoleLog": "error"
    },
    "plugins": ["esaCustom"]
}
